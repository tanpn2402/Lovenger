<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="@color/left"
                />

            <solid
                android:color="@color/left"
                />

            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"
                />

            <corners
                android:topLeftRadius="5dp"
                android:bottomRightRadius="10dp"
                android:bottomLeftRadius="5dp"
                android:topRightRadius="10dp"
                />
        </shape>
    </item>
</layer-list>
